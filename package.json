    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.2.0",
    "@types/chai-as-promised": "^7.1.6",
    "@types/ip": "^1.1.3",
    "@types/lodash": "^4",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=22.14.1",
    "@types/seedrandom": "^3",
    "axios": "^1.8.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cspell": "^8.19.2",
    "eslint": "^9.22.0",
    "ethers": "^6.4.0",
    "globals": "^16.0.0",
    "hardhat": "^2.22.19",
    "hardhat-gas-reporter": "^2.2.3",
    "ip": "2.0.1",
    "js-combinatorics": "^2.1.2",
    "lodash": "^4.17.21",
    "seedrandom": "^3.0.5",
    "solhint": "^5.0.5",
    "solidity-coverage": "^0.8.0",
    "ts-node": ">=8.0.0",
    "typechain": "^8.3.0",
    "typescript": ">=4.5.0",
    "typescript-eslint": "^8.31.0"
  },
  "scripts": {
    "compile": "yarn hardhat compile",
    "cleanCompile": "yarn hardhat clean && yarn compile",
    "cspell": "yarn exec cspell .",
    "eslint": "yarn exec eslint .",
    "fullCheck": "yarn compile && yarn lint && yarn eslint",
    "hooks": "git config core.hooksPath .githooks",
    "lint": "yarn solhint 'contracts/**/*.sol'",
    "no-hooks": "git config core.hooksPath .git/hooks",
    "slither": "yarn cleanCompile && slither --hardhat-ignore-compile .",
    "test": "yarn tsc && npx hardhat test",
    "tsc": "yarn exec tsc --noEmit"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.3.0",
    "@openzeppelin/contracts-upgradeable": "^5.2.0",
    "@openzeppelin/hardhat-upgrades": "^3.9.0",
